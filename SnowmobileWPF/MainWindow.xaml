<Window x:Class="SnowmobileWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SnowmobileWPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="0*"/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <ListBox Name="SubscriberList" Margin="10,38,10,10" Grid.ColumnSpan="2" MouseDoubleClick="SubscriberList_MouseDoubleClick"/>
        <Button Name="SearchButton" Content="Search" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="147" Click="SearchButton_Click" Grid.ColumnSpan="2"/>
        <Button Name="CreateDummyButton" Content="Create dummy" HorizontalAlignment="Left" Margin="162,10,0,0" VerticalAlignment="Top" Width="147" Click="CreateDummyButton_Click" Grid.ColumnSpan="2"/>
        <Button Name="DeleteButton" Content="Delete" Grid.ColumnSpan="2" HorizontalAlignment="Left" Margin="314,10,0,0" VerticalAlignment="Top" Width="203" Click="DeleteButton_Click"/>
    </Grid>
</Window>
